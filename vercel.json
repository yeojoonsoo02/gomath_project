{
  "version": 2,
  "projects": [
    {
      "name": "gomath-web",
      "source": "apps/web",
      "framework": "nextjs"
    }
  ],
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://gomath-api.railway.app/api",
    "NEXT_PUBLIC_GRAPHQL_URL": "https://gomath-api.railway.app/graphql"
  }
}